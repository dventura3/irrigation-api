{
  "iot": "https://iotdb.org/pub/iot#",
  "iot-unit": "https://iotdb.org/pub/iot-unit#",
  "vocab": "http://www.example.org/vocab#",
  "dbpedia": "http://dbpedia.org/resource/",
  "sensorID": "iot:uuid",
  "sensorName": "iot:name",
  "sensorDescription": "iot:description",
  "sensorData": {
      "@id": "vocab:hasValue",
      "@type" : "http://www.w3.org/2001/XMLSchema#double"
  },
  "sensorUnit": {
    "@id" : "iot:unit",
    "@type" : "@id"
  },
  "timestamp": {
      "@id" : "vocab:hasTimestamp",
      "@type" : "http://www.w3.org/2001/XMLSchema#dateTime"
  },
  "MoistureSensor": "vocab:MoistureSensor",
  "madeObservation": "vocab:madeObservation",
  "observationResult": "vocab:outputObservation",
  "Moisture": "dbpedia:Moisture"
}