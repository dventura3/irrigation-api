@prefix ex: <http://example.org/>.
@prefix dbpedia: <http://dbpedia.org/resource/>.
@prefix st: <http://www.mystates.org/states#>.
@prefix unit: <http://data.nasa.gov/qudt/owl/unit#>.


ex:MySensorTemp	a ex:TemperatureSensor.
ex:MySensorLight a ex:LightSensor.
ex:MySensorMoisture a ex:MoistureSensor.


{
	ex:MySensorTemp ex:observes _:temp.
	ex:MySensorLight ex:observes _:light.
	ex:MySensorMoisture ex:observes _:moisture.
} a st:State.

_:temp 	a dbpedia:Temperature;
				ex:hasValue _:val;
				ex:hasUnit unit:DegreeCelsius.

_:light a dbpedia:Light;
				ex:hasValue _:val.

_:moisture a dbpedia:Moisture;
				ex:hasValue _:val.